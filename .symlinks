#!/bin/csh
if ( ! -e index.html ) then
  ln -s README.html index.html
endif
if ( ! -e CBFlib.tar.gz ) then
  ln -s ../CBFlib_0.7.6.tar.gz CBFlib.tar.gz
endif
if ( ! -e template_adscquantum315_3072x3072.cbf ) then
  ln -s examples/template_adscquantum315_3072x3072.cbf template_adscquantum315_3072x3072.cbf
endif
if ( ! -e template_adscquantum4_2304x2304.cbf ) then
  ln -s examples/template_adscquantum4_2304x2304.cbf template_adscquantum4_2304x2304.cbf
endif
if ( ! -e template_mar345_2300x2300.cbf ) then
  ln -s examples/template_mar345_2300x2300.cbf template_mar345_2300x2300.cbf
endif
foreach file (*)
  if ( -d $file ) then
    if ( -e $file/.symlinks ) then
      (cd $file; source .symlinks)
    endif
  endif
end
